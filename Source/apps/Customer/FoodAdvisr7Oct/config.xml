<?xml version='1.0' encoding='utf-8'?>
<widget id="foodadvisr.testfilght.002" version="2.2.5" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>FoodAdvisr</name>
    <description>
      FoodAdvisr
  </description>
    <author email="sreekar.saladi@hellocurry.com" href="http://advisrtech.com">
      AdvisrTech Ltd
  </author>
    <preference name="orientation" value="portrait" />
    <content src="index.html" />
    <access origin="*" />
    <access launch-external="yes" origin="tel:*" />
    <feature name="http://api.phonegap.com/1.0/camera" />
    <feature name="http://api.phonegap.com/1.0/geolocation" />
    <feature name="http://api.phonegap.com/1.0/network" />
    <gap:config-file mode="add" parent="ITSAppUsesNonExemptEncryption" platform="ios">
        <false />
    </gap:config-file>
    <preference name="orientation" value="portrait" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="android-minSdkVersion" value="7" />
    <preference name="android-installLocation" value="auto" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="UIWebViewBounce" value="true" />
    <engine name="ios" spec="^4.5.2" />
    <engine name="android" spec="^6.3.0" />
    <engine name="browser" spec="^5.0.1" />
    <plugin name="cordova-plugin-device" spec="~1.1.5" />
    <plugin name="cordova-plugin-geolocation" spec="~2.4.3">
        <variable name="GEOLOCATION_USAGE_DESCRIPTION" value="We use your location to help improve our data accuracy" />
    </plugin>
    <plugin name="cordova-plugin-network-information" spec="1.3.3" />
    <plugin name="cordova-plugin-statusbar" spec="~2.2.0" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.2" />
    <plugin name="phonegap-plugin-barcodescanner" spec="~6.0.5">
        <variable name="CAMERA_USAGE_DESCRIPTION" value="Scan QR-Codes" />
    </plugin>
    <plugin name="cordova-plugin-camera" spec="^2.4.1">
        <variable name="PHOTOLIBRARY_USAGE_DESCRIPTION" value="App would like to access the library to store screenshot results." />
        <variable name="CAMERA_USAGE_DESCRIPTION" value="we use camera to scan barcode of products to give accurrate results." />
    </plugin>
</widget>
